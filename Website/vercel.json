{
  "version": 2,
  "buildCommand": "cd "Source Code/Code" && npm run build",
  "outputDirectory": "Source Code/Code",
  "rewrites": [
    { "source": "/about", "destination": "/about.html" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/favicon.ico",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        }
      ]
    },
    {
      "source": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|woff|woff2))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "trailingSlash": false,
  "builds": [
    {
      "src": "Source Code/Code/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "Source Code/Code"
      }
    }
  ],
  "routes": [
    {
      "src": "/about",
      "dest": "/about.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "devCommand": "cd "Source Code/Code" && npm run dev",
  "installCommand": "cd "Source Code/Code" && npm install"
}